						<retsid>101436</retsid>
						<example></example>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element	name="KickOutTimestamp"
						type="xs:dateTime"
						minOccurs="0"
						maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						The timestamp that the contingency
						reason failed.
					</xs:documentation>
					<xs:appinfo>
						<retsid>101437</retsid>
						<example></example>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
