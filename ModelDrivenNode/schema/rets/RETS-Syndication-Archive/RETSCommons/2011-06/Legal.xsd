	<xs:complexType name="ContingencyType">
		<xs:sequence>
			<xs:element	name="Description"
						type="commons:ContingencyDescriptionEnum"
						minOccurs="0"
						maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
