			<xsd:documentation>Cancellation number.  Example: 5432265</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DocumentationNumber" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Documentation number.  Example: TSH123</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
