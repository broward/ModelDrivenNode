				<xsd:element ref="DeckName" minOccurs="0"/>
				<xsd:element ref="DeparturePoint" minOccurs="0"/>
				<xsd:element ref="ArrivalPoint" minOccurs="0"/>
				<xsd:element ref="TourCode" minOccurs="0"/>
				<xsd:element ref="TourName" minOccurs="0"/>
				<xsd:element ref="GroundTransportation" minOccurs="0"/>
				<xsd:element ref="PropertyCode" minOccurs="0"/>
				<xsd:element ref="PropertyName" minOccurs="0"/>
				<xsd:element ref="LocationInfo" minOccurs="0"/>
				<xsd:element ref="NumberOfPersons" minOccurs="0"/>
