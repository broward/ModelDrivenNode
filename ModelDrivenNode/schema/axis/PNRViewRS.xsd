			<xsd:documentation>Deck name.  Example: Deck1</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="GroundTransportation" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Ground transportation.  Example: Bus</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RatePlan" type="xsd:string">
